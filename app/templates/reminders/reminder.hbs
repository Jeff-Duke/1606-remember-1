<div class="reminder">
  <h3 class="spec-reminder-title">{{model.title}}</h3>

  <p class="spec-reminder-date">
    {{model.date}}
  </p>

  {{#if model.notes}}
    <p class="spec-reminder-notes">
      {{model.notes}}
    </p>
  {{/if}}

  <button {{action 'editReminder'}}>edit</button>

</div>

{{outlet}}
